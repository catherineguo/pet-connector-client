{{#each checklists as |checklist|}}
  {{log checklist}}
  <div class="card">
    <div class="card-body" data-id="{{checklist.id}}">
      <h4 class="card-title">Title: {{checklist.title}}</h4>
      <p class="card-text">Start date: {{checklist.start_date}}
        <br>
        End date: {{checklist.end_date}}
        <br>
        Pet Name: {{checklist.pet_name}}
        <br>
        Instructions: {{checklist.instructions}}</p>
      <button type="button" class="btn btn-primary">Delete</button>
    </div>
  </div>
{{/each}}
